<h1 class="text-center">Add a new cocktail</h1>
<div class="container">
<%= simple_form_for(@cocktail) do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Submit cocktail", class: "btn btn-primary" %>
  <% end %>
<br><br><br>
  <a href="/cocktails" class="btn-home" role="button" aria-disabled="true"><p>Home</p></a>
</div>
